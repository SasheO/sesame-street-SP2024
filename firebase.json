{
  "hosting": {
   
    "rewrites": [ {
      "source": "/bigben",
      "function": {
        "functionId": "bigben",
        "region": "us-central1",  
        "pinTag": true         
      }
    },
    {
      "source": "/embedded_google_maps",
      "function": "app"
    },
    {
      "source": "/embedded_google_search",
      "function": "app"
    }
   ]
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint"
      ]
    }
  ]
}
